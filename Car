import java.util.Scanner;


public class car {
    private String name;
    private int model;
    private int price;
    public void setData(String n,int m,int p){
        name=n;
        model=m;
        price=p;
    }
    public void showData(){
        System.out.println("Name:"+name+"Model"+model+"price"+price);
    }

}
class Test
{
    public static void main(String[] args) {
        Scanner kb=new Scanner(System.in);
        String n=kb.next();
        int c=kb.nextInt();
        int p=kb.nextInt();
        car thar=new car();
        thar.getdata(n,c,p);
        thar.showDAta();


    }
}
